<div class="container">
  <div class="row">
    <div class="col-md-12">
      <table class="table table-responsive">
          <tbody>
            <tr>
              <td>タグリスト</td>
              <% @tag_list.each do |tag| %>
                <td>
                  <%= link_to posts_path(tag_id: tag.id), class: "text-dark" do %>
                    <%= tag.name %>
                  <% end %>
                </td>
              <% end %>
            </tr>
        </tbody>
      </table>
    </div>
  </div>
  <div class="row">
    <div class="col-md-2">
      <%= render 'sidebar' %>
    </div>

    <div class="col-md-4">
      <div class="row">
        <div class="col">
          <h4><%= @post_nameの投稿一覧 %></h4>
        </div>
      </div>
      <%= render 'post_index', posts: @posts %>
    </div>

    <div class="col-md-6">
      <%= render 'map' %>
    </div>
  </div>
</div>